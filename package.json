{
  "name": "code-quick-refer",
  "displayName": "code-quick-refer",
  "description": "copy a reference of code quickly.",
  "publisher": "wikichen",
  "version": "0.0.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamyyou/code-quick-refer.git"
  },
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-quick-refer.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "code-quick-refer.generateReferences",
        "title": "复制对象引用"
      },
      {
        "command": "code-quick-refer.copyLineNumber",
        "title": "复制行号"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "code-quick-refer.generateReferences",
          "when": "editorTextFocus",
          "group": "navigation@10"
        },
        {
          "command": "code-quick-refer.copyLineNumber",
          "when": "editorTextFocus",
          "group": "navigation@11"
        }
      ]
    },
    "keybindings": [
      {
        "command": "code-quick-refer.generateReferences",
        "key": "alt+d",
        "mac": "alt+d",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "package:vsix": "pnpm dlx @vscode/vsce package",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "esbuild": "^0.27.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
